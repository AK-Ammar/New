Sub TestHighlightMatchingCombinations()
    ' Sample data
    Dim valuesA() As Variant
    Dim valuesB() As Variant
    valuesA = Range("E8:E18").Value
    valuesB = Range("F8:F15").Value
    
    ' Generate combinations
    Dim result() As Variant
    result = GenerateCombinations(valuesA, valuesB)
    
    ' Debug print to check the content of the result array
    Debug.Print "Result Array:"
    Dim i As Long
    For i = LBound(result) To UBound(result)
        Debug.Print "Combination " & i & ": " & result(i)(1) & ", " & result(i)(2)
    Next i
    
    ' Call HighlightMatchingCombinations with populated result array
    HighlightMatchingCombinations valuesA, valuesB, result
End Sub
